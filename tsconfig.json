{
  "compilerOptions": {
    "target": "es5" ,
    "module": "commonjs" ,
    "lib": [
      "ES5" ,
      "ES6"
    ] ,
    "sourceMap": true ,
    "outDir": "./dist" ,
    "strict": true ,
    "baseUrl": "./" ,
    "esModuleInterop": true ,
    "experimentalDecorators": true ,
    "emitDecoratorMetadata": true
  } ,
  "exclude": [
    "node_modules"
  ] ,
  "include": [
    "src/**/*.ts"
  ]
}